# Olivine Frontend Environment Variables

# API Endpoints
VITE_API_URL=http://localhost:8080
VITE_GRAPHQL_URL=http://localhost:8080/graphql

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_KEY=your_supabase_anon_key

# OAuth Client IDs
VITE_DROPBOX_CLIENT_ID=your_dropbox_client_id
VITE_GOOGLE_DRIVE_CLIENT_ID=your_google_drive_client_id

# Application Settings
VITE_APP_NAME=Olivine
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_DROPBOX=true
VITE_ENABLE_GOOGLE_DRIVE=true
VITE_ENABLE_VERSIONING=true
VITE_ENABLE_BRANCHING=true
VITE_ENABLE_AI_AGENTS=true

# UI Settings
VITE_THEME=dark
VITE_DEFAULT_LOCALE=en-US
VITE_FALLBACK_LOCALE=en-US
